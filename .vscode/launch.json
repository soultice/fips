  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'fips'",
      "cargo": {
        "args": [
          "build",
          "--bin=fips",
          "--package=fips",
        ],
        "filter": {
          "name": "fips",
          "kind": "bin"
        }
      },
      "args": [
        "--config=./nconfig-test"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'fips'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=fips",
          "--package=fips"
        ],
        "filter": {
          "name": "fips",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
